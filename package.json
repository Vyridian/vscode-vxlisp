      "extensions": [".vxlisp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "vxlisp",
      "scopeName": "source.vxlisp",
      "path": "./syntaxes/vxlisp.tmLanguage.json"
    }]
  }
